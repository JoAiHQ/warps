{
  "protocol": "warp:3.0.0",
  "name": "Allowance: Revoke",
  "title": {
    "en": "Revoke Allowance"
  },
  "description": {
    "en": "Revoke an allowance and withdraw all remaining funds back to the owner."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Revoke"
      },
      "address": "INJECT:ALLOWANCE_SC",
      "func": "revoke",
      "gasLimit": 10000000,
      "inputs": [
        {
          "name": "Blockchain",
          "as": "chain",
          "bot": "The blockchain network.",
          "type": "string",
          "position": "chain",
          "source": "field",
          "required": true
        },
        {
          "name": "Allowance ID",
          "as": "allowance_id",
          "description": {
            "en": "The ID of the allowance to revoke"
          },
          "type": "uint64",
          "position": "arg:1",
          "source": "field",
          "required": true
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Allowance revoked. Remaining funds returned to owner."
    }
  }
}
