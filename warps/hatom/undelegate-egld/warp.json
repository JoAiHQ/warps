{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "Hatom: Undelegate eGold",
  "title": { "en": "Unstake eGold from Hatom", "de": "eGold von Hatom entstaken" },
  "description": { "en": "Unstake your eGold (EGLD) you have previously staked with Hatom.", "de": "Entstake dein eGold (EGLD), das du zuvor bei Hatom gestaked hast." },
  "actions": [
    {
      "type": "contract",
      "label": { "en": "Undelegate now", "de": "Jetzt entstaken" },
      "address": "INJECT:PROVIDER",
      "func": "unDelegate",
      "args": [],
      "gasLimit": 10000000,
      "inputs": [
        {
          "name": "value",
          "description": { "en": "Amount of eGold to undelegate.", "de": "Betrag an eGold zum Entstaken." },
          "type": "biguint",
          "position": "arg:1",
          "source": "field",
          "required": true,
          "min": 1,
          "modifier": "scale:18"
        }
      ]
    }
  ],
  "output": {
    "TX_HASH": "out",
    "AMOUNT_EGLD": "transform:() => Number(input:value) / 10 ** 18"
  },
  "ui": "https://raw.githubusercontent.com/JoAiHQ/warps/main/warps/hatom/undelegate-egld/chatapp.dist.html",
  "messages": {
    "success": { "en": "eGold undelegated successfully", "de": "eGold erfolgreich entstaked" }
  },
  "related": [
    "@multiversx:multiversx-staking-claimable-rewards"
  ]
}
