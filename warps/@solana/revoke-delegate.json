{
  "protocol": "warp:3.0.0",
  "chain": "solana",
  "name": "Solana: Revoke Delegate",
  "title": {
    "en": "Revoke Token Delegate",
    "de": "Token-Delegierten widerrufen"
  },
  "description": {
    "en": "Revoke a delegate's approval to spend tokens from your token account using the Solana Token Program. Removes any previously granted spending permission.",
    "de": "Widerrufe die Genehmigung eines Delegierten, Token aus deinem Token-Konto ueber das Solana Token Program auszugeben. Entfernt jede zuvor erteilte Ausgabegenehmigung."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Revoke Delegate",
        "de": "Delegierten widerrufen"
      },
      "address": "TokenkegQfeZyiNwAJbNbGKPFXCWuBvf9Ss623VQ5DA",
      "func": "revoke",
      "abi": "{\"instructions\":{\"revoke\":{\"discriminator\":[5],\"args\":[],\"accounts\":[{\"address\":\"{{USER_ATA:MINT_ADDRESS}}\",\"writable\":true,\"signer\":false},{\"address\":\"{{USER_WALLET}}\",\"writable\":false,\"signer\":true}]}}}",
      "inputs": [
        {
          "name": "Mint Address",
          "as": "MINT_ADDRESS",
          "type": "address",
          "position": "input:0",
          "source": "field",
          "required": true,
          "description": {
            "en": "The mint address of the token.",
            "de": "Die Mint-Adresse des Tokens."
          }
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Delegate revoked successfully.",
      "de": "Delegierter erfolgreich widerrufen."
    }
  },
  "defaultChain": "solana"
}
