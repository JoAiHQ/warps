{
  "protocol": "warp:3.0.0",
  "name": "JoAi: Changelog Entry Add",
  "title": {
    "en": "Add Changelog Entry",
    "de": "Changelog-Eintrag hinzufügen"
  },
  "description": {
    "en": "Add a new entry to the changelog.",
    "de": "Einen neuen Eintrag zum Changelog hinzufügen."
  },
  "actions": [
    {
      "type": "collect",
      "label": {
        "en": "Add Entry",
        "de": "Eintrag hinzufügen"
      },
      "inputs": [
        {
          "name": "Type",
          "as": "type",
          "type": "string",
          "options": [
            "feature",
            "fix",
            "improvement",
            "update"
          ],
          "source": "field",
          "required": true
        },
        {
          "name": "Title",
          "as": "title",
          "type": "string",
          "source": "field",
          "required": true,
          "description": {
            "en": "e.g. 'MultiversX Integration'",
            "de": "z.B. 'MultiversX Integration'"
          }
        },
        {
          "name": "Version",
          "as": "version",
          "type": "string",
          "source": "field",
          "required": false,
          "description": {
            "en": "e.g. '0.1.0'",
            "de": "z.B. '0.1.0'"
          }
        },
        {
          "name": "Description",
          "as": "description",
          "type": "string",
          "source": "field",
          "required": true,
          "description": {
            "en": "e.g. 'Added skills for retrieving account information, transaction information...'",
            "de": "z.B. 'Skills für den Abruf von Kontoinformationen hinzugefügt...'"
          }
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Changelog entry added.",
      "de": "Changelog-Eintrag hinzugefügt."
    }
  }
}
