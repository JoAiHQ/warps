{
  "protocol": "warp:3.0.0",
  "name": "JoAi: User Onboarding",
  "title": {
    "en": "Let's Get Started!"
  },
  "description": {
    "en": "Help us tailor your experience with AI agents by telling us a bit about yourself."
  },
  "actions": [
    {
      "type": "collect",
      "label": {
        "en": "Submit Info"
      },
      "destination": {
        "url": "INJECT:API_BASE/warps/flow",
        "method": "POST"
      },
      "inputs": [
        {
          "name": "Telegram Username",
          "as": "telegramUsername",
          "type": "string",
          "source": "field",
          "required": true
        },
        {
          "name": "What is your biggest time-waster?",
          "as": "biggestTimeWaster",
          "description": {
            "en": "Tasks you spend the most time on and feel should be automated. Or just something you don't like doing."
          },
          "type": "string",
          "source": "field",
          "required": true
        },
        {
          "name": "Primary Goal with AI Agents",
          "as": "primaryGoalAI",
          "description": {
            "en": "Do you have a primary goal for using AI agents?"
          },
          "type": "string",
          "source": "field",
          "required": true
        },
        {
          "name": "Technical Comfort Level",
          "as": "technicalComfortLevel",
          "description": {
            "en": "1 = Beginner (ChatGPT level) ... 5 = Advanced (code APIs/bots)."
          },
          "type": "string",
          "source": "field",
          "required": true
        },
        {
          "name": "What blockchain/crypto apps do you use daily or weekly?",
          "as": "blockchainCryptoApps",
          "type": "string",
          "source": "field",
          "required": true
        },
        {
          "name": "Which AI tools do you currently pay for?",
          "as": "aiToolsPaid",
          "type": "string",
          "source": "field",
          "required": true
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Your onboarding information has been submitted successfully! We'll be in touch soon."
    }
  },
  "defaultChain": "multiversx"
}
