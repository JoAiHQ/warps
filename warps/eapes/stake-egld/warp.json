{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "EAPES: Stake eGold",
  "title": "Stake eGold with EAPES",
  "description": "Where capital meets culture, we power the chain and strengthen the tribe. Stake eGold to secure the network and earn rewards.",
  "actions": [
    {
      "type": "contract",
      "label": "Stake now",
      "address": "INJECT:PROVIDER",
      "func": "delegate",
      "args": [],
      "gasLimit": 10000000,
      "inputs": [
        {
          "name": "value",
          "description": "Amount of eGold to delegate.",
          "type": "biguint",
          "position": "value",
          "source": "field",
          "required": true,
          "min": 1,
          "modifier": "scale:18"
        }
      ]
    },
    {
      "type": "link",
      "label": "Learn more",
      "description": "Find more information about staking eGold.",
      "url": "https://multiversx.com/egld"
    }
  ],
  "output": {
    "TX_HASH": "out",
    "AMOUNT_EGLD": "transform:() => Number(input:value) / 10 ** 18"
  },
  "ui": "https://raw.githubusercontent.com/JoAiHQ/warps/main/warps/eapes/stake-egld/chatapp.dist.html",
  "messages": {
    "success": "eGold staked successfully"
  },
  "related": [
    "@multiversx:multiversx-staking-claimable-rewards",
    "@multiversx:multiversx-staking-redelegate-all"
  ]
}