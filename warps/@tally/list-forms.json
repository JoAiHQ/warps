{
  "protocol": "warp:3.0.0",
  "name": "Tally: List Forms",
  "title": {
    "en": "List forms",
    "de": "Formulare auflisten"
  },
  "description": {
    "en": "List your Tally forms.",
    "de": "Liste deine Tally-Formulare auf."
  },
  "vars": {
    "ACCESS_TOKEN": "env:TALLY_ACCESS_TOKEN|Your Tally Personal Access Token."
  },
  "actions": [
    {
      "type": "collect",
      "label": "List Forms",
      "destination": {
        "url": "https://api.tally.so/forms",
        "method": "GET",
        "headers": {
          "Authorization": "Bearer {{ACCESS_TOKEN}}"
        }
      },
      "inputs": []
    }
  ],
  "output": {
    "FORMS": "out.forms",
    "COUNT": "out.forms.length"
  },
  "messages": {
    "success": {
      "en": "Found {{COUNT}} form(s).",
      "de": "{{COUNT}} Formular(e) gefunden."
    }
  }
}
