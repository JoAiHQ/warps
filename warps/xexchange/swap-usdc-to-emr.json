{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap USDC to EMR",
  "title": {
    "en": "Swap USDC to EMR",
    "de": "USDC gegen EMR tauschen"
  },
  "description": {
    "en": "Swap USDC to EMR tokens on xExchange Devnet.",
    "de": "USDC gegen EMR Token auf xExchange Devnet tauschen."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to EMR",
        "de": "Gegen EMR tauschen"
      },
      "address": "erd1qqqqqqqqqqqqqpgqhlnuj8dpru7zs4mm49pudr2uk53w74px0n4snd2495",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/EMR"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:abis/pair.abi.json",
      "inputs": [
        {
          "name": "USDC Asset",
          "as": "asset",
          "description": {
            "en": "USDC tokens to swap.",
            "de": "USDC Token zum Tauschen."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/USDC"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum EMR to receive.",
            "de": "Minimales EMR zum Erhalten."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} EMR",
      "de": "Tausch abgeschlossen. Erhalten {{AMOUNT_OUT}} EMR"
    }
  },
  "output": {
    "OUT": "out.1",
    "AMOUNT_OUT": "out.1.amount"
  }
}