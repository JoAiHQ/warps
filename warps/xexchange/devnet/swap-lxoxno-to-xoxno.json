{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap LXOXNO to XOXNO",
  "title": {
    "en": "Swap LXOXNO to XOXNO"
  },
  "description": {
    "en": "Swap LXOXNO to XOXNO tokens on xExchange Devnet."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to XOXNO"
      },
      "address": "erd1qqqqqqqqqqqqqpgqzaxrk7rkxpus3l59v8jt3w5g5jac6x570n4s7fw9hp",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/XOXNO"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:xexchange/pair.abi.json",
      "inputs": [
        {
          "name": "LXOXNO Asset",
          "as": "asset",
          "description": {
            "en": "LXOXNO tokens to swap."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/LXOXNO"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum XOXNO to receive."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ],
      "output": {
        "OUT": "out.1",
        "AMOUNT_OUT": "out.1.amount"
      }
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} XOXNO"
    }
  }
}
