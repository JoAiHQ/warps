{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap WEGLD to CRT",
  "title": {
    "en": "Swap WEGLD to CRT"
  },
  "description": {
    "en": "Swap WEGLD to CRT tokens on xExchange Devnet."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to CRT"
      },
      "address": "erd1qqqqqqqqqqqqqpgqh7ds4z4cswktn2z7z7pfct4sa3qn2dsp0n4sas2vus",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/CRT"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:xexchange/pair.abi.json",
      "inputs": [
        {
          "name": "WEGLD Asset",
          "as": "asset",
          "description": {
            "en": "WEGLD tokens to swap."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/WEGLD"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum CRT to receive."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ],
      "output": {
        "OUT": "out.1",
        "AMOUNT_OUT": "out.1.amount"
      }
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} CRT"
    }
  }
}
