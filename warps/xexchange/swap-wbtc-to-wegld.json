{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap WBTC to WEGLD",
  "title": {
    "en": "Swap WBTC to WEGLD",
    "de": "WBTC gegen WEGLD tauschen"
  },
  "description": {
    "en": "Swap WBTC to WEGLD tokens on xExchange Devnet.",
    "de": "WBTC gegen WEGLD Token auf xExchange Devnet tauschen."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to WEGLD",
        "de": "Gegen WEGLD tauschen"
      },
      "address": "erd1qqqqqqqqqqqqqpgqll98p9940f6u5erf4dje5r0f94g4jctc0n4s48vmz2",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/WEGLD"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:abis/pair.abi.json",
      "inputs": [
        {
          "name": "WBTC Asset",
          "as": "asset",
          "description": {
            "en": "WBTC tokens to swap.",
            "de": "WBTC Token zum Tauschen."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/WBTC"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum WEGLD to receive.",
            "de": "Minimales WEGLD zum Erhalten."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} WEGLD",
      "de": "Tausch abgeschlossen. Erhalten {{AMOUNT_OUT}} WEGLD"
    }
  },
  "output": {
    "OUT": "out.1",
    "AMOUNT_OUT": "out.1.amount"
  }
}