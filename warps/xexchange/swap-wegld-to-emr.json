{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap WEGLD to EMR",
  "title": {
    "en": "Swap WEGLD to EMR",
    "de": "WEGLD gegen EMR tauschen"
  },
  "description": {
    "en": "Swap WEGLD to EMR tokens on xExchange Devnet.",
    "de": "WEGLD gegen EMR Token auf xExchange Devnet tauschen."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to EMR",
        "de": "Gegen EMR tauschen"
      },
      "address": "erd1qqqqqqqqqqqqqpgqs9uf6k9zlhrf6wymxm5m5spt0qmvtajt0n4s2vfk9u",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/EMR"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:abis/pair.abi.json",
      "inputs": [
        {
          "name": "WEGLD Asset",
          "as": "asset",
          "description": {
            "en": "WEGLD tokens to swap.",
            "de": "WEGLD Token zum Tauschen."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/WEGLD"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum EMR to receive.",
            "de": "Minimales EMR zum Erhalten."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} EMR",
      "de": "Tausch abgeschlossen. Erhalten {{AMOUNT_OUT}} EMR"
    }
  },
  "output": {
    "OUT": "out.1",
    "AMOUNT_OUT": "out.1.amount"
  }
}