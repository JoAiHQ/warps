{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap EMR to USDC",
  "title": {
    "en": "Swap EMR to USDC",
    "de": "EMR gegen USDC tauschen"
  },
  "description": {
    "en": "Swap EMR to USDC tokens on xExchange Devnet.",
    "de": "EMR gegen USDC Token auf xExchange Devnet tauschen."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to USDC",
        "de": "Gegen USDC tauschen"
      },
      "address": "erd1qqqqqqqqqqqqqpgqhlnuj8dpru7zs4mm49pudr2uk53w74px0n4snd2495",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/USDC"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:abis/pair.abi.json",
      "inputs": [
        {
          "name": "EMR Asset",
          "as": "asset",
          "description": {
            "en": "EMR tokens to swap.",
            "de": "EMR Token zum Tauschen."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/EMR"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum USDC to receive.",
            "de": "Minimales USDC zum Erhalten."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} USDC",
      "de": "Tausch abgeschlossen. Erhalten {{AMOUNT_OUT}} USDC"
    }
  },
  "output": {
    "OUT": "out.1",
    "AMOUNT_OUT": "out.1.amount"
  }
}