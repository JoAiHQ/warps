{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap USDC to MEX",
  "title": {
    "en": "Swap USDC to MEX"
  },
  "description": {
    "en": "Swap USDC to MEX tokens on xExchange Devnet."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to MEX"
      },
      "address": "erd1qqqqqqqqqqqqqpgqlmwaf993zd0ukglm5m59w4fd8mwp95us0n4sl223md",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/MEX"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:abis/pair.abi.json",
      "inputs": [
        {
          "name": "USDC Asset",
          "as": "asset",
          "description": {
            "en": "USDC tokens to swap."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/USDC"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum MEX to receive."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ],
      "output": {
        "OUT": "out.1",
        "AMOUNT_OUT": "out.1.amount"
      }
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} MEX"
    }
  }
}