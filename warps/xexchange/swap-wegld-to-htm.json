{
  "protocol": "warp:3.0.0",
  "chain": "multiversx",
  "name": "xExchange Devnet: Swap WEGLD to HTM",
  "title": {
    "en": "Swap WEGLD to HTM"
  },
  "description": {
    "en": "Swap WEGLD to HTM tokens on xExchange Devnet."
  },
  "actions": [
    {
      "type": "contract",
      "label": {
        "en": "Swap to HTM"
      },
      "address": "erd1qqqqqqqqqqqqqpgqnp4wf8nsd0pfz957fe2n8gzpksycxc790n4sqvv74v",
      "func": "swapTokensFixedInput",
      "args": [
        "string:INJECT:TOKEN/HTM"
      ],
      "gasLimit": 25000000,
      "abi": "INJECT:abis/pair.abi.json",
      "inputs": [
        {
          "name": "WEGLD Asset",
          "as": "asset",
          "description": {
            "en": "WEGLD tokens to swap."
          },
          "type": "asset",
          "position": "transfer",
          "source": "field",
          "required": true,
          "options": [
            "INJECT:TOKEN/WEGLD"
          ]
        },
        {
          "name": "Minimum Amount Out",
          "description": {
            "en": "Minimum HTM to receive."
          },
          "type": "biguint",
          "position": "arg:2",
          "source": "field",
          "default": "1",
          "min": 1
        }
      ]
    }
  ],
  "messages": {
    "success": {
      "en": "Swap completed. Received {{AMOUNT_OUT}} HTM"
    }
  },
  "output": {
    "OUT": "out.1",
    "AMOUNT_OUT": "out.1.amount"
  }
}